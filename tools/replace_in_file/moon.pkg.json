{
  "pre-build": [
    {
      "command": "moon -C external/derive_json_schema run external/derive_json_schema/main.mbt -- tools/replace_in_file/schema.mbt -o tools/replace_in_file/schema_generated.mbt && moonfmt -w tools/replace_in_file/schema_generated.mbt",
      "input": "schema.mbt",
      "output": "schema_generated.mbt"
    }
  ],
  "import": [
    "moonbitlang/maria/file",
    "moonbitlang/maria/tool",
    "moonbitlang/maria/internal/fs",
    "moonbitlang/maria/internal/path",
    "moonbitlang/maria/internal/fuzzy"
  ],
  "test-import": [
    "moonbitlang/async",
    "moonbitlang/maria/tools/read_file",
    "moonbitlang/maria/internal/mock",
    "moonbitlang/maria/agent",
    "moonbitlang/maria/model",
    "moonbitlang/maria/openai",
    "moonbitlang/maria/internal/pino"
  ]
}
